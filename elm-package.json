{
    "version": "5.2.0",
    "summary": "Type inference for Elm, written in Elm.",
    "repository": "https://github.com/joonazan/elm-type-inference.git",
    "license": "Apache-2.0",
    "source-directories": [
        "src"
    ],
    "exposed-modules": [
        "Infer",
        "Infer.Type",
        "Infer.Monad",
        "Infer.Scheme",
        "Infer.Expression"
    ],
    "dependencies": {
        "elm-community/list-extra": "7.0.1 <= v < 8.0.0",
        "elm-lang/core": "5.1.1 <= v < 6.0.0",
        "elm-lang/html": "2.0.0 <= v < 3.0.0",
        "folkertdev/elm-state": "3.0.0 <= v < 4.0.0",
        "Bogdanp/elm-ast": "10.0.1 <= v < 11.0.0",
        "elm-community/graph": "5.0.1 <= v < 6.0.0",
        "elm-community/intdict" : "2.1.0 <= v < 3.0.0"
    },
    "dependency-sources": {
        "Bogdanp/elm-ast": {
            "url": "https://github.com/MajronMan/elm-ast.git",
            "ref": "pattern_match"
        }
    },
    "elm-version": "0.18.0 <= v < 0.19.0"
}
